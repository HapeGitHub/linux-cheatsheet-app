{
  "Introduction": {
    "description": "Overview of Linux, Bash, distributions, and CLI history.",
    "commands": [
      {
        "name": "What is Bash?",
        "command": "echo $BASH_VERSION",
        "description": "Shows the installed Bash version.",
        "examples": ["echo $BASH_VERSION"]
      },
      {
        "name": "Current Shell",
        "command": "echo $SHELL",
        "description": "Displays the default shell for the user.",
        "examples": ["echo $SHELL"]
      }
    ]
  },
  "Basic Commands": {
    "description": "Common day-to-day commands.",
    "commands": [
      {
        "name": "List files",
        "command": "ls",
        "description": "Lists directory contents.",
        "examples": ["ls", "ls -la"]
      },
      {
        "name": "Change directory",
        "command": "cd",
        "description": "Changes the current working directory.",
        "examples": ["cd /home/user", "cd .."]
      },
      {
        "name": "Print working directory",
        "command": "pwd",
        "description": "Prints the current directory path.",
        "examples": ["pwd"]
      }
    ]
  }
}
